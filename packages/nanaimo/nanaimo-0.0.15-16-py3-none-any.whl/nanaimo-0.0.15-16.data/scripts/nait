"""
The Nanaimo CLI.
"""

import argparse
import sys


def _make_parser() -> argparse.ArgumentParser:
    """
    Provided for documentation purposes only (sphinx-argparse). Do not use.
    """
    import nanaimo.cli
    return nanaimo.cli._make_parser()


if __name__ == "__main__":
    import nanaimo.cli
    sys.exit(nanaimo.cli.main())
