{
  "name": "domain.com",
  "version": "0.0.1",
  "description": "my application domain.com",
  "gokku": {
    "domain_name": "domain.com",
    "runtime": "python",
    "auto_restart": true,
    "env": {
      "ASSEMBLY_ENV": "Production",
      "ASSEMBLY_PROJECT": "default"
    },
    "scripts": {
      "release": [
        "asm-admin db:sync",
        "asm setup"
      ]
    },    
    "run": {
      "web": "wsgi:app"
    }
  }
}