#!/home/acoddington/Documents/Projects/jirafs/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'jirafs==1.17.5','console_scripts','jirafs'
__requires__ = 'jirafs==1.17.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jirafs==1.17.5', 'console_scripts', 'jirafs')()
    )
